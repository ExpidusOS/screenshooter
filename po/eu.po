#: ../lib/screenshooter-dialogs.c:528
msgid "<span weight=\"bold\" stretch=\"semiexpanded\">Region to capture</span>"
msgstr ""
"<span weight=\"bold\" stretch=\"semiexpanded\">Kapturatzeko eremua</span>"

