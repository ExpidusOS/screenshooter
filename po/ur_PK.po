#, c-format
msgid "Screenshot-%d.png"
msgstr "Screenshot-%d.png"

#: ../lib/screenshooter-dialogs.c:493
#, c-format
msgid "%.2fKb of %.2fKb"
msgstr "%.2fKb of %.2fKb"

#: ../lib/screenshooter-dialogs.c:577
