msgid "Version information"
msgstr "Version information"

#: ../src/main.c:140
#, c-format
msgid "Conflicting options: --%s and --%s cannot be used at the same time.\n"
msgstr "Conflicting options: --%s and --%s cannot be used at the same time.\n"

#: ../src/main.c:142
#, c-format
